/* Icon Component - Heroicons Integration */

/* #region Helper Classes */
.icon-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(120px, 1fr));
    gap: var(--spacing-16);
    align-items: center;
  }
  
  .icon-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: var(--spacing-8);
  }
  
  .icon-container {
    border: 1px solid var(--color-border-default);
    border-radius: var(--spacing-8);
  }
  
  .code-small {
    font-size: 12px;
  }

/* #endregion */

/* #region Heroicon Base Classes */

  /* Base heroicon class - 24px solid icons by default */
  .heroicon {
    display: inline-block;
    width: var(--spacing-24);
    height: var(--spacing-24);
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center;
    flex-shrink: 0;
    /* Use mask to make background images inherit currentColor */
    -webkit-mask-size: contain;
    -webkit-mask-repeat: no-repeat;
    -webkit-mask-position: center;
    mask-size: contain;
    mask-repeat: no-repeat;
    mask-position: center;
    background-color: currentColor;
  }
  
  /* Size modifiers */
  .heroicon-16 {
    width: var(--spacing-16);
    height: var(--spacing-16);
  }
  
  .heroicon-20 {
    width: var(--spacing-20);
    height: var(--spacing-20);
  }

  /* Legacy support for browsers that don't support mask */
  .heroicon.no-mask-support {
    background-color: transparent;
    /* Filter approach as fallback - converts dark colors to currentColor */
    filter: 
      brightness(0) 
      saturate(100%) 
      invert(var(--icon-invert, 0)) 
      sepia(var(--icon-sepia, 0)) 
      hue-rotate(var(--icon-hue-rotate, 0deg)) 
      brightness(var(--icon-brightness, 1)) 
      contrast(var(--icon-contrast, 1));
  }

/* #endregion */

